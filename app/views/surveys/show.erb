<div class="container">
  <h1><%= @survey.survey_name %></h1>
  <br>
  <ol>
    <% @survey.questions.each do |question| %>
      <li><h2><%= question.content %></h2></li>
      <ol>
      <% question.answers.each do |answer| %>
        <li><%= answer.content %></li>
      <%end%>
      </ol>
    <%end%>
  </ol>
</div>
